  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.maxus.tsp.platform.service.dao.car.RedisBackUpDao">

	<resultMap type="com.maxus.tsp.platform.service.model.car.RedisTboxInfo"
		id="RedisTboxInfoMap">
		<id column="ID" property="id" />
		<result column="tboxSn" property="tboxSn" />
		<result column="value" property="value" />
	</resultMap>
	
	<!-- rds_car_remote_ctrl_resp -->
    <!--仅查询sn是否存在-->
    <select id="checkTboxInfoIndb_rds_car_remote_ctrl_resp" resultType="java.lang.Integer">
        SELECT
        COUNT(0)
        FROM rds_car_remote_ctrl_resp
        WHERE tboxSn=#{tboxSn}
    </select>

    <!--查询并获取记录-->
    <select id="getRecordIndb_rds_car_remote_ctrl_resp" resultMap="RedisTboxInfoMap">
        SELECT
        id,tboxSn,value
        FROM rds_car_remote_ctrl_resp
        WHERE tboxSn=#{tboxSn}
    </select>
    
    <!--删除记录-->
    <delete id="deleteTboxInfodb_rds_car_remote_ctrl_resp">
        delete from rds_car_remote_ctrl_resp where tboxSn=#{tboxSn}
    </delete>
    
    <!--更新对应表数据-->
    <update id="updateTboxInfodb_rds_car_remote_ctrl_resp">
    	update rds_car_remote_ctrl_resp set value=#{value}
    	where tboxSn = #{tboxSn}
    </update>
    
    <!--插入新数据-->
    <insert id="insertTboxInfodb_rds_car_remote_ctrl_resp">
		insert
		into
		rds_car_remote_ctrl_resp(
		tboxSn,
		value)

		values(
		#{tboxSn},
		#{value})
	</insert>
	
	<!-- 获取全部数据 -->
	<select id="getAllRecordsIndb_rds_car_remote_ctrl_resp" resultMap="RedisTboxInfoMap">
		SELECT
		id,tboxSn,value
		FROM rds_car_remote_ctrl_resp
	</select>
	
	<!-- 删除全部数据 -->
	<select id="deleteAllRecordsIndb_rds_car_remote_ctrl_resp">
		DELETE FROM rds_car_remote_ctrl_resp
	</select>
	
	<!-- 插入数据 -->
	<insert id="insertAllRecordsIndb_rds_car_remote_ctrl_resp" parameterType="java.util.Map">
		INSERT INTO rds_car_remote_ctrl_resp (tboxSn,value)
		VALUES
		<foreach collection="currentrecords.entrySet()" item ="value" index ="key" separator=",">
			(#{key},#{value})
		</foreach>
	</insert>






